<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="semana7.elemento">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Actualizar - Eliminar"/>
            <Entry x:Name="txtNombre" Placeholder="Nombre"></Entry>
            <Entry x:Name="txtUsuario" Placeholder="Usuario"></Entry>
            <Entry x:Name="txtContrasenia" Placeholder="Contraseña"></Entry>
            <Button x:Name="bntActualizar" Text="Actulizar" Clicked="bntActualizar_Clicked"></Button>
            <Button x:Name="btnEliminar" Text="Eliminar" Clicked="btnEliminar_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>